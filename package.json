    "autoprefixer": "^10.1.0",
    "contentful": "^8.1.5",
    "hamburger-react": "^2.4.0",
    "next": "10.0.4",
    "postcss": "^8.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.6",
