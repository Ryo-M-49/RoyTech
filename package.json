    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.*"
    ],
    "setupFiles": [
      "./tests/setupTest.ts"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^14.1.2",
